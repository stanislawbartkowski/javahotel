<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

	<jython>
		<import>from testpack import testmail</import>
		<method>testmail.dialogaction({0},{1})</method>
	</jython>

	<form>
		<field id="bignumber" type="decimal" afterdot="4" />
		<field id="no" type="int" />
		<field id="email" type="email" />
	</form>

	<list id="blist">
		<columns>
			<column id="id" type="int" />
			<column id="blob_person" />
			<column id="blob_comment" />
			<column id="blob_key" />
			<column id="blob_date" type="date" />
		</columns>
	</list>

    <list id="blistnowrap" nowrap="">
        <columns>
            <column id="id" type="int" />
        </columns>
    </list>

</dialog>