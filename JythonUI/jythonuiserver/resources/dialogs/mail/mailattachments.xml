<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

	<before />

	<jython>
		<import>from mail import maillist</import>
		<method>maillist.dialogattach({0},{1})</method>
	</jython>

	<list id="listattach" toolbartype="empty" displayname="@listofattachments">
		<columns>
			<column id="filename" displayname="@filename" actionid="download" />
			<column id="realm" hidden="" />
			<column id="key" hidden="" />
		</columns>
	</list>


</dialog>