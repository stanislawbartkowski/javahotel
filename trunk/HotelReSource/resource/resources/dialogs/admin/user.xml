<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">
	<before />

	<parent>admin/users.xml</parent>
	
	<jython>
		<import>from adminpack import admin</import>
		<method>admin.userelemaction({0},{1})</method>
	</jython>
	

	<form>
		<field id="name" displayname="@name" readonlychange="" notempty="" />
		<field id="descr" displayname="@description" />
	</form>

	<checklist id="perm" displayname="@permissions">
	</checklist>

</dialog>