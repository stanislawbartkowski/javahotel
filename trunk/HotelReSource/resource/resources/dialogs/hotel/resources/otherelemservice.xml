<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com" displayname="@otherservice">

	<before />

	<parent>?otherservices.xml</parent>

	<types>hotel/typesenum.xml</types>

	<jython>
		<method>otherservices.elemserviceaction({0},{1})</method>
	</jython>

	<form>
		<field id="name" displayname="@name" readonlychange="" notempty="" />
		<field id="descr" displayname="@description" />
		<field id="vat" type="custom:vattax" displayname="@vatname"
			notempty="" />
	</form>

</dialog>