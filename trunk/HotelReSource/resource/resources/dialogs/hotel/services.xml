<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

	<before />
			
	<jython>
		<import>from hotelpack import services</import>
		<method>services.serviceaction({0},{1})</method>
	</jython>


	<list id="services" displayname="@listofservices" elemformat="hotel/elemservice.xml">
		<columns>
			<column id="name" displayname="@name" />
			<column id="descr" displayname="@description" />
			<column id="noperson" type="int" displayname="@nopersons" />
			<column id="vat" hidden=""/>
			<column id="vatname" displayname="@vatname"/>
		</columns>
	</list>

</dialog>