<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

<before/>

        <jython>
                <import>from hotelpack.reservation import pdflist</import>
                <method>pdflist.billprint({0},{1})</method>
        </jython>
   
        <buttons>
                <button id="savepdf" displayname="Save PDF"
                        actiontype="JYESNO_MESSAGE" actionparam="Save this PDF ?"
                        actionparam1="@confirmation" />
        </buttons>                        
        
   <form>
     <field id="html" displayname="Print" type = "html"/>
     <field id="pdfbillno" hidden=""/>
     <field id="tempkey" hidden="" />
     <field id="download" displayname="Click to download and print" type="download"/>
     <field id="pdf_comment" displayname="Description to save" />
   </form>

</dialog>