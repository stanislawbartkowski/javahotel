<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com" displayname = "Reservation panel" >

	<before />
        
        <clearleft />
        
	<jython>
		<import>from hotelpack.reservation import reservation</import>
		<method>reservation.reservationaction({0},{1})</method>
	</jython>
        

	<dateline id="reservation" lineid="name"
		defafile="empty" colno="10">
		<columns>
			<column id="name" displayname="@name" />
			<column id="noperson" displayname="no" type="int" />
			<column id="descr" displayname="@description" />
		</columns>
		<files>
			<filedef id="empty" file="hotel/reservation/roomempty.html" />
			<filedef id="reserved" file="hotel/reservation/reservation.html" />
			<filedef id="stay" file="hotel/reservation/stay.html" />			
		</files>

	</dateline>
</dialog>