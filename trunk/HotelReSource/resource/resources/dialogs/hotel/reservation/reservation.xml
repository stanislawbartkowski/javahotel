<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

	<before />

	<jython>
		<import>from hotelpack import reservation</import>
		<method>reservation.reservationaction({0},{1})</method>
	</jython>

	<dateline id="reservation" displayname="Reservation" lineid="name" colno="10">
		<columns>
			<column id="name" displayname="@name"/>
			<column id="noperson" displayname="no" type="int" />
			<column id="descr" displayname="@description" />
		</columns>
	</dateline>
</dialog>