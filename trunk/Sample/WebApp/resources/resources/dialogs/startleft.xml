  <button id="checkbignumbers" displayname="Big numbers" actiontype="JMAIN_DIALOG" actionparam="checkbignumbers.xml" />  
  <button id="listnext" displayname="Different lists" header="" />
  <button id="listwithcustom" displayname="List with custom" actiontype="JMAIN_DIALOG" actionparam="listwithcustom.xml" />
  <button id="dialogattach" displayname="Dialog with attachment" actiontype="JMAIN_DIALOG" actionparam="dialogattach.xml" />
  <button id="dialogdisc" displayname="Dialog with disclosure" actiontype="JMAIN_DIALOG" actionparam="dialogdisc.xml" />
  <button id="dialognext" displayname="List with custom search" actiontype="JMAIN_DIALOG" actionparam="dialoglist1.xml" />
