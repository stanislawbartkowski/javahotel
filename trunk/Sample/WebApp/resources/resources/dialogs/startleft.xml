<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

<before/>
 
 <jython>
   <import>from testpack import main</import>
   <method>main.dialogaction({0},{1})</method>
 </jython>  

<leftstack>
  <button id="h" displayname = "Alerts" header="" />  
  <button id="okmessage" displayname="Ok Message" actiontype="JOK_MESSAGE" actionparam="Hello" actionparam1="Title" />
  <button id="errmessage" displayname="Err Message" actiontype="JERROR_MESSAGE" actionparam="Terrible error" actionparam1="Fatal" />
  <button id="upbutton" displayname="Check button" actiontype="JMAIN_DIALOG" actionparam="buttonafter.xml" />
  <button id="enablebutton" displayname="Enable/disable button" actiontype="JMAIN_DIALOG" actionparam="buttonenable.xml" />
  <button id="htmldialog" displayname="Html dialog" actiontype="JMAIN_DIALOG" actionparam="htmldialog.xml" />
  <button id="h1" displayname = "Status" header="" />  
  <button id="statustext" displayname="Status tekst" actiontype="JMAIN_DIALOG" actionparam="statusup.xml" />
  <button id="listfooter" displayname="List footer" actiontype="JMAIN_DIALOG" actionparam="listdeffooter.xml" />
  <button id="listnext" displayname="Different lists" header="" />
  <button id="listwithcustom" displayname="List with custom" actiontype="JMAIN_DIALOG" actionparam="listwithcustom.xml" />
  <button id="dialogattach" displayname="Dialog with attachment" actiontype="JMAIN_DIALOG" actionparam="dialogattach.xml" />
</leftstack>

 <form>
   <field id="glob1" />
   <field id="globbool" type="bool" notempty="" />
   <field id="globint" type="int" displayname="Integer value" />
   <field id="globlong" type="int" displayname="Long value" />
   <field id="globdecimal" type="decimal" afterdot="2" displayname="Decimal" />
   <field id="globdate" type="date" displayname="Date" />
   <field id="globdatetime" type="datetime" displayname="Date and time" />   
 </form>  

 
</dialog>