  <button id="htmldialog" displayname="Html dialog" actiontype="JMAIN_DIALOG" actionparam="htmldialog.xml" />
  <button id="authidedialog" displayname="Autohide dialog" actiontype="JUP_DIALOG" actionparam="autodialog.xml" />
  <button id="modelessdialog" displayname="Modeless dialog" actiontype="JUP_DIALOG" actionparam="nextdialog.xml" />  
  <button id="jsdialog" displayname="Dialog with JS" actiontype="JUP_DIALOG" actionparam="jsdialog.xml" />  
