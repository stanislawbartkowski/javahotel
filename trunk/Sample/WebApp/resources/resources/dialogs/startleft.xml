  <button id="dialogarea" displayname="Area chart" actiontype="JMAIN_DIALOG" actionparam="dialogarea.xml" />
  <button id="dialogscatter" displayname="Scatter chart" actiontype="JMAIN_DIALOG" actionparam="dialogscatter.xml" />
  <button id="dialogcircle" displayname="Circle chart" actiontype="JMAIN_DIALOG" actionparam="dialogcircle.xml" />
  <button id="x" displayname="Mail" header="" />  
  <button id="sendmail" displayname="Send mail" actiontype="JMAIN_DIALOG" actionparam="sendmail.xml" />  
</leftstack>

 <form>
   <field id="glob1" />
   <field id="globbool" type="bool" notempty="" />
   <field id="globint" type="int" displayname="Integer value" />
   <field id="globlong" type="int" displayname="Long value" />
   <field id="globdecimal" type="decimal" afterdot="2" displayname="Decimal" />
   <field id="globdate" type="date" displayname="Date" />
   <field id="globdatetime" type="datetime" displayname="Date and time" />   
 </form>  

 
</dialog>