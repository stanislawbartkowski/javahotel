[alias]
EDITLIST:editlist
CLEAR1:clear1button
CLEAR2:clear2button
ROW0://tr[@__gwt_row='0']
UTEXT1://tr[@__gwt_row='0']/td[2]//*[@value="1"]
UCELL1://tr[@__gwt_row='0']/td[2]//input
[start]
call:waitforwindow
waitFor:#EDITLIST
click:#EDITLIST
waitFor:#ADDBEFORE
sleep:3
-- clear
click:#CLEAR1
call:clickyes
sleep:2
click:#CLEAR2
call:clickyes
sleep:5
-- add to the first panel
click:#ADDBEFORE
call:clickyes
waitFor:#ROW0
waitFor:#UTEXT1
waitFor:#UCELL1
type:#UCELL1|2
click:#ADDBEFORE
call:clickyes
sleep:5
isPresent://*[@value="xxxx"]
isPresent://*[@value="1"]
isPresent://*[@value="2"]
click:#REMOVEROW
call:clickyes
sleep:1
waitForNot://*[@value="1"]
