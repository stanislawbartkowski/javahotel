[alias]
CHUNK:id=list chunk
TABLEROW://tbody/tr[3]/td/div
[check2]
click:#SHOWITEM
waitFor:name
verEqual:name|2: name
verEqual:id|2
click:#OK
[start]
call:waitforwindow
---
click:#CHUNK
waitFor:#TABLEROW
sleep:1
click:#TABLEROW
sleep:2
-- select and show window
call:check2 
-- click next page
sleep:1
click:#NEXTLISTBUTTON
waitFor:#TABLEROW
click:#TABLEROW
-- show again
click:#SHOWITEM
waitFor:name
verEqual:name|22: name
verEqual:id|22
click:#OK
-- click last
sleep:1
click:#LASTLISTBUTTON
waitFor:#TABLEROW
click:#TABLEROW
-- show again
click:#SHOWITEM
waitFor:name
verEqual:name|82: name
verEqual:id|82
click:#OK
--
waitFor:#FIRSTLISTBUTTON
click:#FIRSTLISTBUTTON
--
--/html/body/table/tbody/tr[2]/td[2]/table/tbody/tr[2]/td/table/tbody/tr[3]/td/table/tbody/tr[2]/td/div/table/tbody/tr[2]/td/table/thead/tr/th/p
-- /html/body/table/tbody/tr[2]/td[2]/table/tbody/tr[2]/td/table/tbody/tr[3]/td/table/tbody/tr[2]/td/div/table/tbody/tr[2]/td/table/thead/tr/th[2]
sleep:1
-- now click sort column
click:#HEADERSORT2
waitFor:#TABLEROW
sleep:2
click:#TABLEROW
-- show 11
click:#SHOWITEM
waitFor:name
verEqual:name|11: name
verEqual:id|11
click:#OK
sleep:1
-- now remove sorting
click:#TOOL
sleep:1
waitFor:#REMOVESORT
click:#REMOVESORT
waitFor:#TABLEROW
sleep:1
-- should be again 2
call:check2
-- now filter
click:#FILTRLIST
waitFor:id
type:id|5
type:id-1|50
click:#SETFILTER
sleep:2
-- wait for data
waitFor:#TABLEROW
click:#TABLEROW
click:#SHOWITEM
waitFor:#OK
click:#OK
click:#SHOWITEM
waitFor:name
verEqual:name|7: name
verEqual:id|7
click:#OK
--- now click sort (sort and filter)
sleep:1
click:#HEADERSORT2
sleep:1
waitFor:#TABLEROW
click:#TABLEROW
click:#SHOWITEM
waitFor:name
verEqual:name|12: name
verEqual:id|12
click:#OK

