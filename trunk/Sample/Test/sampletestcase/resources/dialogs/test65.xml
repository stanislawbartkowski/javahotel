<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

    <clearleft />
    <clearcentre />

    <jython>
        <import>from testpack import readlist</import>
        <method>readlist.dialogaction({0},{1})</method>
    </jython>

    <list id="list" displayname="List of entries">

        <buttons-addlist>accept,check</buttons-addlist>
        <buttons-validate>accept</buttons-validate>

        <columns>
            <column id="id_name" type="int" footer="" footertype="string"/>
            <column id="name" footer="" footeralign="C"/>
            <column id="date" type="date"/>
            <column id="check" type="bool" editable="" signalbefore="" />
        </columns>
    </list>

</dialog>