<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

    <clearleft />
    <clearcentre />

    <jython>
        <import>from testpack import testxml</import>
        <method>testxml.dialogaction({0},{1})</method>
    </jython>

    <list id="list" displayname="List of entries" width="200px">

        <buttons-addlist>accept,check</buttons-addlist>
        <buttons-validate>accept</buttons-validate>

        <columns>
            <column id="id_name" type="int" footer="" footertype="string" width="40%"/>
            <column id="name" footer="" footeralign="C" editclass="ee"/>
            <column id="date" type="date" editcss="rybka" />
            <column id="check" type="bool" editable="" />
        </columns>
    </list>

</dialog>