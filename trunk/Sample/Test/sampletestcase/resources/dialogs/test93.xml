<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

    <jython>
        <import>from testpack import testchart</import>
        <method>testchart.dialogaction({0},{1})</method>
    </jython>
    
    <form>
        <field id="glob1" />
        <field id="globbool" type="bool" />
        <field id="globint" type="int" displayname="Integer value" />
        <field id="spin2" type="spinner" spinnermin="1" spinnermax="24" />
    </form>
    

    <list id="lista" displayname="List of entries">
        <columns>
            <column id="id" defvalue="changepassword" />
            <column id="name" />
        </columns>
    </list>
    
    <chart id="chart" displayname="List of entries">
        <columns>
            <column id="id" type="int" />
            <column id="name" />
        </columns>
    </chart>
    

</dialog>