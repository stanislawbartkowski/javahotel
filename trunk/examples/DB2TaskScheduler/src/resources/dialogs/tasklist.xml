<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

 <before/>

 <types>typesenum.xml</types>
 
 <jython>
   <import>from schedpack import sched</import>
   <method>sched.tasklistaction({0},{1})</method>
 </jython>  
 
 <form>
   <field id="datasource" type="custom:datasources" notempty="" signalchange="" />
 </form>  
 

<list id="list" displayname="List of Datasources" elemformat="taskdescr.xml" pagesize="10" >
 <columns>
   <column id="name" displayname="Task name"/>
   <column id="schema" displayname="Schema" />
   <column id="proc" displayname="Stored procedure" />
   <column id="param" displayname="Procedure param" />
   <column id="begintime" displayname="Begin time" type="datetime" />
   <column id="endtime" displayname="End time" type="datetime" />
   <column id="maxi" displayname="Max" type="int" />
   <column id="schedule" displayname="Schedule" />
   <column id="updatetime" displayname="Update time" type="datetime" />
   <column id="remarks" displayname = "Remarks" />
 </columns>
 </list>

 
</dialog>