		<class>com.javahotel.db.hotelbase.jpa.OfferSeasonPeriod</class>
		<class>com.javahotel.db.hotelbase.jpa.OfferPrice</class>
		<class>com.javahotel.db.hotelbase.jpa.OfferSpecialPrice</class>
		<class>com.javahotel.db.hotelbase.jpa.OfferServicePrice</class>
		<class>com.javahotel.db.hotelbase.jpa.InvoiceIssuer</class>
		<class>com.javahotel.db.hotelbase.jpa.Customer</class>
