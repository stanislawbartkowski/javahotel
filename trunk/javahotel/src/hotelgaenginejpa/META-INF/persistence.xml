		<class>com.javahotel.db.hotelbase.jpa.Guest</class>
		<class>com.javahotel.db.hotelbase.jpa.Invoice</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>
			<property name="openjpa.MetaDataFactory" value="jpa(ClasspathScan=build;lib.jar)" />
		</properties>
	</persistence-unit>
</persistence>
