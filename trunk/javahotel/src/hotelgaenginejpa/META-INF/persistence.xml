<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="EJBPersonRealmPU"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.datanucleus.store.appengine.jpa.DatastorePersistenceProvider</provider>
		<non-jta-data-source />
		<class>com.javahotel.db.authentication.jpa.Hotel</class>
		<class>com.javahotel.db.authentication.jpa.Person</class>
		<class>com.javahotel.db.authentication.jpa.GroupD</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>
			<property name="openjpa.MetaDataFactory" value="jpa(ClasspathScan=build;lib.jar)" />
		</properties>
	</persistence-unit>
	<persistence-unit name="HotelPersistance0"
		transaction-type="RESOURCE_LOCAL">
